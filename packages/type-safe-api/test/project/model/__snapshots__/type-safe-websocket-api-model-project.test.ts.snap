    application/json:
      schema:
        $ref: '../schemas/sendNotificationRequest.yaml'
responses: {}
\`\`\`

\`schemas/index.yaml\`:

\`\`\`yaml
SendNotificationRequest:
  $ref: './sendNotificationRequest.yaml'
\`\`\`

\`schemas/sendNotificationRequest.yaml\`:

\`\`\`yaml
type: object
properties:
  someParameter:
    type: string
  anotherParameter:
